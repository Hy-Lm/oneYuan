page {
  margin: 0;
  padding: 0;
  background: #f9f7f7;
}
.content {
  height: 300px;
}
.my_nav {
  width: 100%;
  height: 135px;
  background-color: #0D8600;
  box-sizing: border-box;
}
.my_nav .header {
  padding: 0 30px;
  width: 100%;
  height: 60px;
  font-size: 14px;
  margin-bottom: 37px;
  box-sizing: border-box;
}
.my_nav .header .inform {
  overflow: hidden;
}
.my_nav .header .inform image {
  width: 16px;
  height: 16px;
  float: right;
}
.my_nav .header .inform image:nth-child(2) {
  margin-right: 20px;
}
.my_nav .header .message {
  overflow: hidden;
  color: #ffffff;
}
.my_nav .header .message image {
  width: 52px;
  height: 52px;
  vertical-align: middle;
}
.my_nav .header .message text:nth-child(2) {
  height: 25px;
  font-size: 18px;
  vertical-align: middle;
  margin-left: 30px;
}
.my_nav .header .message text:nth-child(3) {
  float: right;
  margin-top: 14px;
  padding: 2px 10px;
  width: 75px;
  height: 18px;
  border: 1px solid #ffffff;
  border-radius: 10px;
}
.my_nav .bg {
  width: 100%;
  height: 74px;
  opacity: 1;
  background: #f9f7f7;
  border-radius: 50%;
}
.my_nav .list {
  padding: 0 20px;
  margin-top: -80px;
}
.my_nav .list ._ul {
  padding: 20px;
  list-style: none;
  box-sizing: border-box;
  background-color: #FFFFFF;
  height: 74px;
  opacity: 1;
  border-radius: 7px;
}
.my_nav .list ._ul ._li {
  float: left;
  width: 25%;
  text-align: center;
  font-size: 14px;
}
.my_nav .list ._ul ._li ._p {
  color: #000000;
}
.my_nav .list ._ul ._li text {
  color: #7d7d7d;
}
.my_nav .mycar {
  padding: 20px;
}
.my_nav .mycar .my_box {
  height: 94px;
  padding: 28px 40px 25px 15px;
  box-sizing: border-box;
  background-color: #FFFFFF;
}
.my_nav .mycar .my_box .title {
  float: left;
  width: 36px;
  height: 47px;
  opacity: 1;
  font-size: 18px;
  color: #4f4f4f;
  line-height: 22px;
  margin-right: 40px;
}
.my_nav .mycar .my_box .car {
  float: left;
  font-size: 12px;
  padding-top: 5px;
}
.my_nav .mycar .my_box .car .type image {
  width: 19px;
  height: 19px;
  vertical-align: middle;
  margin-right: 5px;
}
.my_nav .mycar .my_box .car .type text {
  vertical-align: middle;
}
.my_nav .mycar .my_box .img {
  float: right;
  margin-top: 10px;
  width: 10px;
  height: 19px;
}

