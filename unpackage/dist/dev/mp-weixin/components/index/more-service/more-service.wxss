page {
  margin: 0;
  padding: 0;
}
.content {
  height: 360px;
}
.navbar_content {
  width: 100%;
  height: 133px;
  background-color: #0D8600;
  box-sizing: border-box;
}
.navbar_content .header {
  padding: 20px 30px;
  width: 100%;
  height: 60px;
  font-size: 14px;
  margin-bottom: 37px;
  box-sizing: border-box;
}
.navbar_content .header .map {
  float: left;
  color: #F1F1F1;
}
.navbar_content .header image {
  width: 16px;
  height: 16px;
  float: right;
  top: 2px;
}
.navbar_content .header .input {
  margin: 0 29px 0 73px;
  height: 20px;
  opacity: 1;
  background: #f1f1f1;
  border-radius: 10px;
  padding-left: 20px;
  position: relative;
}
.navbar_content .header .input image {
  position: absolute;
  width: 10px;
  height: 10px;
  left: 7px;
  top: 5px;
}
.navbar_content .bg {
  width: 100%;
  height: 74px;
  opacity: 1;
  background: #f9f7f7;
  border-radius: 50%;
}
.navbar_content .sel {
  padding-left: 30px;
  margin-top: -50px;
}
.navbar_content .sel image {
  width: 19px;
  height: 19px;
  vertical-align: middle;
}
.navbar_content .sel ._select {
  border: none;
  display: inline-block;
  vertical-align: middle;
  padding: 0 10px;
}
.navbar_content .banner {
  width: 100%;
  height: 162px;
  padding: 20px;
  box-sizing: border-box;
}
.navbar_content .banner image {
  width: 100%;
  height: 100%;
}
.navbar_content .order {
  padding: 0 30px 10px 30px;
  box-sizing: border-box;
}
.navbar_content .order .order_header ._ul {
  padding: 0;
  list-style: none;
}
.navbar_content .order .order_header ._ul ._li {
  float: left;
  width: 33.33%;
  text-align: center;
  height: 17px;
  font-size: 12px;
  color: #000000;
  line-height: 19px;
}
.navbar_content .order .order_header ._ul ._li:nth-child(1) {
  text-align: left;
}
.navbar_content .order .order_header ._ul ._li:nth-child(3) {
  text-align: right;
}
.navbar_content .order .order_header ._ul .pitch {
  color: #0D8600;
}
.navbar_content .order .type {
  width: 100%;
  overflow: hidden;
  padding: 10px 0;
}
.navbar_content .order .type .left {
  float: left;
  width: 71px;
  height: 71px;
  margin-right: 10px;
}
.navbar_content .order .type .left image {
  width: 100%;
  height: 100%;
}
.navbar_content .order .type .right {
  float: right;
  text-align: left;
  width: calc(100% - 81px);
  height: 71px;
}
.navbar_content .order .type .right view:nth-child(1) {
  height: 22px;
  opacity: 1;
  font-size: 16px;
  text-align: left;
  color: #000000;
}
.navbar_content .order .type .right view:nth-child(2) {
  padding: 5px 0 4px 0;
  height: 17px;
  opacity: 1;
  font-size: 12px;
  text-align: left;
  color: #000000;
}
.navbar_content .order .type .right view:nth-child(2) text {
  margin-right: 12px;
}
.navbar_content .order .type .right view:nth-child(3) {
  height: 17px;
  opacity: 1;
  font-size: 12px;
  text-align: left;
  color: #000000;
}
.navbar_content .order .type .right view:nth-child(3) .km {
  float: right;
}

