page {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  opacity: 1;
  background: #f9f7f7;
}
.footer {
  text-align: center;
  position: fixed;
  bottom: 30px;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  width: 315px;
  height: 30px;
  opacity: 1;
  background: #f86259;
  border-radius: 15px;
  font-size: 16px;
  color: #ffffff;
  line-height: 29px;
}
.myLikecar {
  margin: 0 15px;
}
.car_items {
  margin-top: 22px;
  padding: 16px;
  padding-right: 33px;
  box-sizing: border-box;
  width: 100%;
  height: 130px;
  opacity: 1;
  background: #ffffff;
  border-radius: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.car_items > view:nth-child(2) {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.car_items > view:nth-child(2) > view:nth-child(1) text {
  height: 14px;
  font-size: 10px;
  font-weight: 400;
}
.car_items > view:nth-child(2) > view:nth-child(2) {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-right: 35px;
}
.car_items > view:nth-child(2) > view:nth-child(2) text {
  height: 14px;
  font-size: 10px;
  font-weight: 400;
  color: #707070;
}
.car_items > view:nth-child(2) > view:nth-child(2) image {
  width: 14px;
  height: 14px;
  vertical-align: middle;
  margin-right: 5px;
}
.car_items > view:nth-child(1) {
  padding-bottom: 3px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: 1px solid #EDEDED;
}
.car_items > view:nth-child(1) > view:nth-child(1) > view:nth-child(1) {
  height: 25px;
  font-size: 18px;
  font-weight: 700;
  color: #000000;
  margin-bottom: 5px;
}
.car_items > view:nth-child(1) > view:nth-child(1) > view:nth-child(2) {
  height: 17px;
  font-size: 12px;
  font-weight: 400;
  color: #4e4e4e;
}
.car_items > view:nth-child(1) > view:nth-child(1) > view:nth-child(3) {
  height: 17px;
  font-size: 12px;
  font-weight: 500;
  color: #4e4e4e;
}
.car_items > view:nth-child(1) > view:nth-child(2) image {
  width: 115px;
  height: 73px;
}

