page {
  margin: 0;
  padding: 0;
}
.content {
  height: 360px;
}
.navbar_content {
  width: 100%;
  height: 133px;
  background-color: #396a3a;
  box-sizing: border-box;
}
.navbar_content .header {
  padding: 20px 15px;
  width: 100%;
  height: 60px;
  font-size: 14px;
  margin-bottom: 37px;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.navbar_content .header .map {
  height: 22px;
  opacity: 1;
  font-size: 16px;
  font-weight: 400;
  color: #ffffff;
}
.navbar_content .header image {
  width: 20px;
  height: 20px;
}
.navbar_content .header .input {
  margin: 0 10px 0 5px;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 34px;
  opacity: 1;
  background: #f1f1f1;
  border-radius: 16px;
  padding-left: 20px;
  position: relative;
}
.navbar_content .header .input image {
  position: absolute;
  width: 15px;
  height: 15px;
  left: 14px;
  top: 9px;
}
.navbar_content .bg {
  width: 100%;
  height: 74px;
  opacity: 1;
  background: #f9f7f7;
  border-radius: 50%;
}
.navbar_content .addcart {
  padding: 0 30px 9px 30px;
  margin-top: -100px;
  margin-left: 30px;
  margin-right: 30px;
  box-sizing: border-box;
  height: 83px;
  opacity: 1;
  background: #accb69;
  border-radius: 7px;
  box-shadow: 1px 2px 3px 0px rgba(0, 0, 0, 0.16);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.navbar_content .addcart > view:nth-child(3) image {
  width: 10px;
  height: 19px;
}
.navbar_content .addcart > view:nth-child(2) > view:nth-child(1) {
  margin-bottom: 4px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.navbar_content .addcart > view:nth-child(2) > view:nth-child(1) image {
  width: 19px;
  height: 19px;
  margin-right: 7px;
}
.navbar_content .addcart > view:nth-child(2) > view:nth-child(1) text {
  font-size: 12px;
  color: #000000;
}
.navbar_content .addcart > view:nth-child(2) > view:nth-child(2) {
  font-size: 12px;
  color: #ffffff;
}
.navbar_content .addcart > view:nth-child(1) {
  width: 36px;
  height: 47px;
  opacity: 1;
  font-size: 18px;
  color: #ffffff;
  line-height: 22px;
}
.navbar_content .banner {
  padding: 0 21px;
  text-align: center;
  height: 160px;
}
.navbar_content .hint {
  padding: 2px 30px;
  height: 29px;
  line-height: 29px;
  opacity: 0.39;
  background: #fff0c9;
}
.navbar_content .hint image {
  width: 18px;
  height: 17px;
  opacity: 1;
  vertical-align: middle;
}
.navbar_content .hint text {
  margin-left: 18px;
  font-size: 10px;
  opacity: 1;
  vertical-align: middle;
  line-height: 19px;
}

