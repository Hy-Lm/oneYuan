.myMake {
  width: 100vw;
  height: calc(100vh - 45px);
  /* */
  background: #f9f7f7;
}
.myMake .myMakeNav {
  width: calc(100% - 60px);
  height: 50px;
  padding: 0 30px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.myMake .myMakeNav view {
  padding-bottom: 5px;
}
.myMake .myMakList {
  width: calc(100% - 30px);
  padding: 0 15px;
  height: calc(100% - 50px);
  overflow: hidden;
  overflow-y: scroll;
}
.myMake .myMakList .Make {
  width: calc(100% - 30px);
  height: 70px;
  padding: 10px 15px;
  border-radius: 10px;
  margin: 15px 0;
  background: #f1f1f1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.myMake .myMakList .Make .MakeBox view {
  font-size: 14px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.myMake .myMakList .Make .MakeBox view .business view {
  padding: 2px 10px;
  margin-right: 10px;
  border-radius: 8px;
  color: #fff;
}
.myMake .myMakList .Make .garden {
  width: 60px;
  height: 60px;
  border-radius: 50%;
  background: #fff;
  font-size: 14px;
  text-align: center;
  line-height: 60px;
}
.myMake .myMakList .Make .garden view {
  -webkit-transform: rotate(-30deg);
          transform: rotate(-30deg);
}

