}
.insurance .assess-list-item:nth-child(1) > view:nth-child(2) {
  margin-right: 5px;
}
.insurance .assess-list-item:nth-child(1) > view:nth-child(2) ._select {
  border: none;
  background-color: #FFFFFF;
}
.insurance .assess-list-item image {
  width: 7px;
  height: 12px;
}
.insurance .assess-list-item input {
