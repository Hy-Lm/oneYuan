.insurance {
  margin-top: 7px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  height: 332px;
  opacity: 1;
  background: #ffffff;
  box-shadow: 0px 0px 6px 0px rgba(0, 0, 0, 0.16);
  padding: 15px;
  padding-top: 45px;
  padding-bottom: 0;
