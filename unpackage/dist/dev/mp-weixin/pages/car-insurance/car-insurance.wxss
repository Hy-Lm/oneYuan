.mask {
  z-index: 1;
  width: 100%;
  overflow: hidden;
  height: 100%;
  opacity: 0.17;
  background: #afafaf;
  border: 1px solid #707070;
  position: fixed;
  top: 0;
  left: 0;
}
.mask-model {
  z-index: 2;
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  padding: 0 8px;
  box-sizing: border-box;
  opacity: 1;
  background: #ffffff;
  border-radius: 14px;
  width: 331px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 195px;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.mask-model > view:nth-child(6) {
  width: 25px;
  height: 25px;
  -webkit-transform: translateY(60px);
          transform: translateY(60px);
}
.mask-model > view:nth-child(6) image {
  width: 100%;
  height: 100%;
}
.mask-model > view:nth-child(1) {
  width: 50px;
  height: 50px;
}
.mask-model > view:nth-child(1) image {
  width: 100%;
  height: 100%;
}
.mask-model > view:nth-child(2) {
  opacity: 1;
  font-size: 18px;
  color: #0d8600;
}
.mask-model > view:nth-child(3) {
  opacity: 1;
  font-size: 14px;
  color: #2c2c2c;
}
.mask-model > view:nth-child(4) {
  width: 100%;
  height: 1px;
  opacity: 0.86;
  background: transparent;
  border-bottom: 2px solid #707070;
}
.mask-model > view:nth-child(5) {
  opacity: 1;
  font-size: 14px;
  color: #707070;
}
page {
  overflow: hidden;
  width: 100%;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  opacity: 1;
  background: #f9f7f7;
}
.btn {
  text-align: center;
  position: fixed;
  bottom: 30px;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  width: 315px;
  height: 37px;
  opacity: 1;
  background: #ea4427;
  border-radius: 19px;
  font-size: 14px;
  font-weight: 700;
